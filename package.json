{
  "name": "musicbox",
  "version": "1.3.0",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "FortHell",
    "email": "example@example.example"
  },
  "license": "MIT",
  "description": "A lightweight Electron app for YouTube Music with QoL fixes.",
  "homepage": "https://github.com/FortHell/MusicBox-YTMusic/",
  "devDependencies": {
    "electron": "^36.9.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.forthell.musicbox",
    "productName": "MusicBox",
    "files": [
      "main.js",
      "package.json",
      "node_modules/discord-rpc/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icon.png",
      "maintainer": "FortHell <example@example.example>",
      "category": "Audio"
    }
  },
  "dependencies": {
    "discord-rpc": "^4.0.1"
  }
}
